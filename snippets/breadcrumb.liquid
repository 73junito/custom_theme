.breadcrumb {
  font-size: 16px;
  margin-bottom: 10px;
}

.breadcrumb ul {
  list-style: none;
  margin: 0;
  padding: 0;
}

.breadcrumb li {
  display: inline-block;
  margin-right: 10px;
}

.breadcrumb li:last-child {
  margin-right: 0;
}

.breadcrumb a {
  color: #555;
  text-decoration: none;
}

.breadcrumb a:hover {
  text-decoration: underline;
}

.breadcrumb span {
  color: #999;
}
